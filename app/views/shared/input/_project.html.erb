<%= render 'shared/edit_sub_section' do %>
    <%= project.text_field :title, value: project.object.title, placeholder: "Title" %><br/>
    <%= project.text_field :url, value: project.object.url, placeholder: "Link to the project" %><br/>
    <%= project.text_area :stack, value: project.object.techstack, placeholder: "Skills used" %><br/>
    <%= project.text_area :description, value: project.object.description, placeholder: "Project description" %><br/>
    <%= render 'shared/input/buttons/delete', type: project %>
<% end %>
